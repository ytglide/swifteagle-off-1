---
import Layout from '../layouts/Layout.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';

const structuredData = {
  "@context": "https://schema.org",
  "@type": "AboutPage",
  "name": "About Us - SwiftEagle Technology",
  "description": "Learn about SwiftEagle Technology, our mission, team, and commitment to delivering exceptional IT solutions.",
  "url": "https://www.swifteagletechnology.com/about",
  "mainEntity": {
    "@type": "Organization",
    "name": "SwiftEagle Technology",
    "description": "Leading IT solutions company delivering custom software development, web applications, and digital transformation services",
    "foundingDate": "2020",
    "address": {
      "@type": "PostalAddress",
      "addressLocality": "Kathmandu",
      "addressCountry": "Nepal"
    },
    "contactPoint": {
      "@type": "ContactPoint",
      "telephone": "+977-9705518377",
      "contactType": "customer service"
    }
  }
};
